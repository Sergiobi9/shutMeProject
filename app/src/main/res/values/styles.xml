<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/happy</item>
        <item name="colorPrimaryDark">@color/gray</item>
        <item name="colorAccent">@color/happy</item>
    </style>

    <style name="Widget.BottomNavigationView14" parent="@style/Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/nunito</item>
    </style>

    <style name="ThemeOverlay.MySwitch" parent="">
        <item name="colorControlActivated">@color/happy</item>
        <item name="colorSwitchThumbNormal">@color/gray</item>
        <item name="android:colorForeground">#42221f1f</item>
    </style>

</resources>